  "bugs": {
    "url": "https://github.com/Ferum-bot/Ethereum-Rock-Paper-Scissors/issues"
  },
  "homepage": "https://github.com/Ferum-bot/Ethereum-Rock-Paper-Scissors#readme",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@abraham/reflection": "^0.11.0",
