{
  "name": "ethereum-rock-paper-scissors",
  "version": "1.0.0",
  "description": "Popular game rock-paper-scissors powered by Solidity and Blockchain",
  "main": "index.js",
  "scripts": {
    "setup-ethereum-node": "npx hardhat node",
    "interact": "npx hardhat run src/interact/interact.ts",
